<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">
	<ScrollView
		android:layout_marginTop="30dp"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
<LinearLayout
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
   <android.support.v7.widget.CardView
	   android:id="@+id/english"
	   android:layout_marginLeft="10dp"
	   android:layout_marginRight="10dp"
	   android:layout_width="match_parent"
	   android:layout_height="match_parent"
	   android:layout_marginBottom="30dp"
	   app:cardCornerRadius="0dp"
	   app:cardElevation="4dp"
	   app:contentPadding="10dp"
	   app:cardUseCompatPadding="true"
	   app:cardPreventCornerOverlap="false"
	   app:cardBackgroundColor="@color/colorPrimary">
  <RelativeLayout
	  android:id="@+id/englishLayout"
	  android:layout_width="match_parent"
	  android:layout_height="wrap_content">
    <TextView
	  android:id="@+id/name"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
	    android:textColor="#ffffff"
		android:textSize="18sp"
		android:text="Name"/>
	  <TextView
		  android:id="@+id/namevalue"
		  android:layout_toRightOf="@+id/name"
		  android:layout_marginLeft="55dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="@color/colorAccent"
		  android:textSize="18sp"
		  android:text="Hazrat Muhammad S.A.W"
		/>
	  <TextView
		  android:id="@+id/meaning"
		  android:layout_below="@+id/namevalue"
		  android:layout_marginTop="23dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="#ffffff"
		  android:textSize="18sp"
		  android:text="Meanings"/>
	  <TextView
		  android:id="@+id/meaningvalue"
		  android:layout_below="@+id/namevalue"
		  android:layout_marginTop="23dp"
		  android:layout_toRightOf="@+id/meaning"
		  android:layout_marginLeft="20dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="@color/colorAccent"
		  android:textSize="18sp"
		  android:text="Variant Used For Mohammad - FounderOf Islam.Praise-worthy; Glorified,Praised, lauded, commended, "/>
	  <TextView
		  android:id="@+id/gender"
		  android:layout_below="@+id/meaningvalue"
		  android:layout_marginTop="22dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="#ffffff"
		  android:textSize="18sp"
		  android:text="Gender"/>
	  <TextView
		  android:id="@+id/gendervalue"
		  android:layout_below="@+id/meaningvalue"
		  android:layout_marginTop="22dp"
		  android:layout_toRightOf="@+id/gender"
		  android:layout_marginLeft="42dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="@color/colorAccent"
		  android:textSize="18sp"
		  android:text="Male"/>
	  <TextView
		  android:id="@+id/origin"
		  android:layout_below="@+id/gendervalue"
		  android:layout_marginTop="22dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="#ffffff"
		  android:textSize="18sp"
		  android:text="Origin"/>
	  <TextView
		  android:id="@+id/originvalue"
		  android:layout_below="@+id/gendervalue"
		  android:layout_marginTop="22dp"
		  android:layout_toRightOf="@+id/origin"
		  android:layout_marginLeft="55dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="@color/colorAccent"
		  android:textSize="18sp"
		  android:text="Arabic"/>
	  <TextView
		  android:id="@+id/lucky"
		  android:layout_below="@+id/originvalue"
		  android:layout_marginTop="22dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="#ffffff"
		  android:textSize="18sp"
		  android:text="Lucky No."/>
	  <TextView
		  android:id="@+id/luckyvalue"
		  android:layout_below="@+id/originvalue"
		  android:layout_marginTop="22dp"
		  android:layout_toRightOf="@+id/origin"
		  android:layout_marginLeft="54dp"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:textColor="@color/colorAccent"
		  android:textSize="18sp"
		  android:text=""/>
	 <ImageView
		 android:id="@+id/fav"
		 android:layout_width="30dp"
		 android:layout_height="30dp"
		 android:layout_below="@+id/luckyvalue"
		 android:layout_centerHorizontal="true"
		 android:layout_marginTop="20dp"
		 android:src="@drawable/favselector"/>
	  <ImageView
		  android:id="@+id/audio"
		  android:layout_width="30dp"
		  android:layout_height="30dp"
		  android:layout_below="@+id/luckyvalue"
		  android:layout_alignParentLeft="true"
		  android:layout_marginTop="20dp"
		  android:src="@drawable/audioselector"/>
	  <ImageView
		  android:id="@+id/share"
		  android:layout_width="30dp"
		  android:layout_height="30dp"
		  android:layout_below="@+id/luckyvalue"
		  android:layout_alignParentRight="true"
		  android:layout_marginTop="20dp"
		  android:src="@drawable/shareselector"/>
	  
	  
	  </RelativeLayout>
   </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
		android:id="@+id/urdu"
		android:layout_marginTop="30dp"
		android:layout_marginLeft="10dp"
		android:layout_marginRight="10dp"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:cardCornerRadius="0dp"
		app:cardElevation="4dp"
		app:contentPadding="10dp"
		app:cardUseCompatPadding="true"
		app:cardPreventCornerOverlap="false"
		app:cardBackgroundColor="@color/colorPrimary">
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<TextView
				android:id="@+id/urduname"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="#ffffff"
				android:textSize="25sp"
				android:layout_alignParentRight="true"
				android:text="نام"/>
			<TextView
				android:id="@+id/urdunamevalue"
				android:layout_toLeftOf="@+id/urduname"
				android:layout_marginRight="63dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/colorAccent"
				android:textSize="25sp"
				android:text=""
			/>
			<TextView
				android:id="@+id/urdumeaning"
				android:layout_below="@+id/urdunamevalue"
				android:layout_width="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_height="wrap_content"
				android:textColor="#ffffff"
				android:textSize="25sp"
				android:text="مطلب"/>
			<TextView
				android:id="@+id/urdumeaningvalue"
				android:layout_below="@+id/urdunamevalue"
				android:layout_toLeftOf="@+id/urdumeaning"
				android:layout_marginRight="47dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/colorAccent"
				android:textSize="25sp"
				android:text=""/>
			<TextView
				android:id="@+id/urdugender"
				android:layout_below="@+id/urdumeaningvalue"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:textColor="#ffffff"
				android:textSize="25sp"
				android:text="جنس"/>
			<TextView
				android:id="@+id/urdugendervalue"
				android:layout_below="@+id/urdumeaningvalue"
				android:layout_toLeftOf="@+id/urdugender"
				android:layout_marginRight="53dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/colorAccent"
				android:textSize="25sp"
				android:text=""/>
			<TextView
				android:id="@+id/urduorigin"
				android:layout_below="@+id/urdugendervalue"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:textColor="#ffffff"
				android:textSize="25sp"
				android:text = "ماخذ"	/>
				<TextView
				android:id="@+id/urduoriginvalue"
				android:layout_below="@+id/urdugendervalue"
				android:layout_toLeftOf="@+id/urduorigin"
				android:layout_marginRight="57dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/colorAccent"
				android:textSize="25sp"
				android:text=""/>
			<TextView
				android:id="@+id/urdulucky"
				android:layout_below="@+id/urduoriginvalue"
				android:layout_width="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_height="wrap_content"
				android:textColor="#ffffff"
				android:textSize="23sp"
				android:layout_marginTop="8dp"
				android:text="لکی نمبر"/>
			<TextView
				android:id="@+id/urduluckyvalue"
				android:layout_below="@+id/urduoriginvalue"
				android:layout_toLeftOf="@+id/urdulucky"
				android:layout_marginRight="42dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/colorAccent"
				android:textSize="27sp"
				android:text=""/>
			<ImageView
				android:id="@+id/urdufav"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_below="@+id/urduluckyvalue"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="20dp"
				android:src="@drawable/favselector"/>
			<ImageView
				android:id="@+id/urdushare"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_below="@+id/urduluckyvalue"
				android:layout_alignParentLeft="true"
				android:layout_marginTop="20dp"
				android:src="@drawable/share2selector"/>
		</RelativeLayout>
	</android.support.v7.widget.CardView>	
		</LinearLayout>
	</ScrollView>
</LinearLayout>
